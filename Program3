import java.util.*; 
public class Factorial {
public static void main(String[] args) {
System.out.println("Enter value of n");
Scanner sc= new Scanner(System.in);
int n= sc.nextInt(); 
int c=1;
if (n<0){
    System.out.println("Error");
}

else{
    for(int i=n; i>0; i--){
        c=c*i;
    }
    System.out.print("Factorial of number is: ");
    System.out.print(c);
}
}
}
